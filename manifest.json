{
  "manifest_version": 3,
  "name": "JSON/YAML to DOCX Converter",
  "version": "1.0",
  "description": "Convert JSON and YAML files to DOCX format",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "ru"
}

